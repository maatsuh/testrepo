{
  "name": "testrepo-backend",
  "version": "1.0.0",
  "description": "Secure Node.js backend with Express, PostgreSQL, Redis, and authentication",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.js\"",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js"
  },
  "keywords": [
    "express",
    "postgresql",
    "redis",
    "jwt",
    "authentication",
    "security",
    "api"
  ],
  "author": "maatsuh",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "postgresql": "^0.0.1",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "redis": "^4.6.12",
    "jsonwebtoken": "^9.1.2",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "validator": "^13.11.0",
    "express-async-errors": "^3.1.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "dotenv-cli": "^7.3.0"
  },
  "optionalDependencies": {
    "bcryptjs": "^2.4.3"
  }
}
